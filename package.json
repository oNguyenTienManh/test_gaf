{
  "name": "epre-gift-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development node server",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node server",
    "start:prod": "pm2 start 'yarn start'",
    "deploy": "yarn install && yarn build && yarn start:prod",
    "lint": "eslint '*/**/*.{js,jsx}' --ignore-path .eslintignore --fix"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.14.3",
    "@material-ui/core": "^4.11.0",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "elliptic": "^6.5.4",
    "express": "^4.17.1",
    "immer": "^9.0.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "minimist": "^1.2.3",
    "next": "^10.2.3",
    "next-build-id": "^3.0.0",
    "next-i18next": "^8.3.0",
    "next-redux-wrapper": "^4.0.1",
    "normalize-package-data": "^3.0.2",
    "prop-types": "^15.7.2",
    "react": "16.11.0",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "16.11.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.4",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "sass": "^1.26.12",
    "sass-loader": "^10.1.1",
    "styled-jsx": "^3.3.0",
    "styled-jsx-plugin-sass": "^1.0.0",
    "validator": "^13.1.1"
  },
  "devDependencies": {
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.10",
    "pm2": "4.5.6",
    "prettier": "^2.0.5"
  },
  "lint-staged": {
    "*.js?(x)": [
      "npm run lint",
      "prettier --write .",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
